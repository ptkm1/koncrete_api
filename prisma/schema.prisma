generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id        Bytes  @id @map("_id") @db.ObjectId
  name      String
  email     String
  phone     String
  company   String
  questions Json[]
  average   Int?
}
